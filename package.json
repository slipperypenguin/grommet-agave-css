{
  "name": "grommet-agave-css",
  "version": "1.8.0",
  "description": "Standalone css for grommet ui library.",
  "main": "build/agavehpe/index.min.css",
  "scripts": {
    "build": "node-sass index.scss build/index.min.css --output-style compressed --include-path ./node_modules",
    "build:agavecss": "node-sass themes/agavecss/index.scss build/agavecss/index.min.css --output-style compressed --include-path ./node_modules",
    "build:agavehpe": "node-sass themes/agavehpe/index.scss build/agavehpe/index.min.css --output-style compressed --include-path ./node_modules",
    "start": "node-sass index.scss build/index.min.css -w --output-style compressed --include-path ./node_modules"
  },
  "keywords": [
    "create-react-app",
    "grommet"
  ],
  "author": "Brady Paterson",
  "license": "MIT",
  "devDependencies": {
    "node-sass": "^4.5.1"
  },
  "dependencies": {
    "grommet": "^1.6.0"
  }
}
